{
  "name": "github-search",
  "version": "1.0.0",
  "description": "Github search",
  "scripts": {
    "build": "webpack --progress --colors --mode=production",
    "start": "webpack-dev-server --config webpack.config.js --progress --colors --mode=development",
    "apollo-download-schema": "npx apollo client:download-schema src/generated/apollo/github-schema.json --endpoint https://api.github.com/graphql --header \"authorization: Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}\"",
    "apollo-codegen": "npx apollo client:codegen src/generated/apollo/ --endpoint https://api.github.com/graphql --header \"authorization: Bearer ${GITHUB_PERSONAL_ACCESS_TOKEN}\" --target typescript --outputFlat",
    "prettier": "npx prettier --write ."
  },
  "author": "Anton Kiselev-Minaev",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "7.10.4",
    "@babel/preset-env": "7.10.4",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@graphql-codegen/cli": "^1.16.0",
    "@graphql-codegen/fragment-matcher": "^1.16.0",
    "@testing-library/react": "^10.4.3",
    "babel-loader": "8.1.0",
    "dotenv-webpack": "^1.8.0",
    "fork-ts-checker-webpack-plugin": "5.0.5",
    "html-webpack-plugin": "4.3.0",
    "prettier": "2.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "apollo-boost": "^0.4.9",
    "graphql": "^15.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "styled-components": "^5.1.1",
    "use-debounce": "^3.4.2",
    "webfontloader": "^1.6.28"
  }
}
